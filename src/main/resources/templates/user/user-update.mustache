{{>layout/header2}}

<h1>내전 유저 수정</h1>
<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="title">유저id</label>
                <input type="text" class="form-control" id="id" value="{{user.userId}}" readonly>
            </div>
            <div class="form-group">
                <label for="title">유저명</label>
                <input type="text" class="form-control" id="userName" value="{{user.userName}}">
            </div>
            <div class="form-group">
                <label for="title">주라인</label>
                <input type="text" class="form-control" id="mainLane" value="{{user.mainLane}}">
            </div>
            <div class="form-group">
                <label for="title">부라인</label>
                <input type="text" class="form-control" id="subLane" value="{{user.subLane}}">
            </div>
            <div class="form-group">
                <label for="title">레이팅</label>
                <input type="number" class="form-control" id="rating" value="{{user.rating}}">
            </div>
        </form>
        <a href="/" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="btn-update">수정 완료</button>
        <button type="button" class="btn btn-danger" id="btn-delete">유저 삭제</button>
    </div>
</div>

{{>layout/footer}}